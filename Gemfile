source "https://rubygems.org"

# Hello! This is where you manage which Jekyll version is used when your site is generated.
# You can add other gems here if you want.

# 加载主题自身的依赖 (如果你是在开发主题)
gemspec

# 插件组
group :jekyll_plugins do
  gem "jekyll-paginate"
  gem "jekyll-seo-tag", "~> 2.7.1"
  gem "jekyll-archives"
  gem "jekyll-sitemap"
end
